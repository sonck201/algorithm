{
  "configurations": [
    {
      "name": "Attach by Process ID",
      "processId": "${command:PickProcess}",
      "protocol": "inspector",
      "request": "attach",
      "type": "node"
    },
    {
      "name": "Algorithm Dijkstra",
      "program": "${workspaceFolder}/src/dijkstra.ts",
      "request": "launch",
      "runtimeArgs": [
        "-r",
        "ts-node/register/transpile-only",
        "-r",
        "tsconfig-paths/register",
      ],
      "type": "pwa-node",
      "outputCapture": "std",
    },
    {
      "name": "Algorithm Bellman-Ford",
      "program": "${workspaceFolder}/src/bellman-ford.ts",
      "request": "launch",
      "runtimeArgs": [
        "-r",
        "ts-node/register/transpile-only",
        "-r",
        "tsconfig-paths/register",
      ],
      "type": "pwa-node",
      "outputCapture": "std",
    },
  ],
  "version": "0.2.0"
}
